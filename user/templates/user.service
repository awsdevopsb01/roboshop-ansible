[Unit]
Description = User Service
[Service]
User=roboshop
Environment=DOCUMENTDB=true
Environment={{ lookup('aws_ssm', 'dev.user.ecache.nldevopsb01_url', region='us-east-1' ) }}
Environment=MONGO_URL={{ lookup('aws_ssm', 'dev.user.mongo.nldevopsb01_url', region='us-east-1' ) }}
ExecStart=/bin/node /app/server.js
SyslogIdentifier=user

[Install]
WantedBy=multi-user.target